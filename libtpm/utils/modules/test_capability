	doTestSetCapFlag "ReadSRKPub" \
	                 $TPM_SET_PERM_FLAGS \
	                 $TPM_PF_READSRKPUB \
	                 $FALSE \
	                 checkTPMReadSRKPub \
	                 $OWNER_PASSWORD

#	doTestSetCapFlag "TPMEstablished" \
