	doTestSetCapFlagOnce "Allow Maintenance" \
	                 $TPM_SET_PERM_FLAGS \
	                 $TPM_PF_ALLOWMAINTENANCE \
	                 $FALSE \
	                 checkTPMAllowMaintenance \
	                 $OWNER_PASSWORD

#	doTestSetCapFlag "PhysicalPresenceLifetimeLock" \
